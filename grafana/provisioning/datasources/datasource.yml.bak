apiVersion: 1

datasources:
  - name: Prometheus
    type: prometheus
    access: proxy
    url: http://monitoring.prometheus:9090
    basicAuth: false
    isDefault: true
    editable: true
  - name: Loki
    type: loki
    access: proxy
    url: http://monitoring.loki:3100
    basicAuth: false
    isDefault: false
    editable: true
  - name: LokiAsPrometheus
    type: prometheus
    access: proxy
    url: http://monitoring.loki:3100/loki
    basicAuth: false
    isDefault: false
    editable: true
